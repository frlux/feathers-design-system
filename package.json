    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^3.10.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-merge-and-include-globally": "^2.1.16"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "airbnb",
      "plugin:vue/essential"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not Explorer > 0",
    "IE 11",
    "not ExplorerMobile > 0",
    "not BlackBerry > 0",
    "not OperaMini all",
    "not OperaMobile > 0"
  ],
  "engines": {
    "node": ">= 8.6.0",
    "npm": ">= 3.0.0"
  },
  "files": [
    "dist/system/*",
    "dist/assets/*"
  ],
  "homepage": "https://fontanalib.org",
  "keywords": [
    "vue design system",
    "design system",
    "vue.js",
    "vue",
    "design",
    "system"
  ],
  "license": "MIT",
  "main": "dist/system/system.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frl-ux/feathers-design-system.git"
  }
}
