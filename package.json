{
  "name": "fontanalib-design-system",
  "version": "0.20.1",
  "private": false,
  "description": "Vue Design System - Fontana Regional Library",
  "author": "amy_actually <awest@fontanalib.org>",
  "scripts": {
    "build": "npm-run-all theo node:build",
    "test": "npm-run-all theo unit",
    "lint": "eslint '**/*.{js,vue}' --cache",
    "build:app": "npm run build",
    "build:docs": "npm-run-all theo styleguide:build",
    "build:system": "npm-run-all theo node:build:system",
    "dev": "webpack-dev-server --inline --config ./build/webpack.dev.conf.js",
    "node:build": "node ./build/build.js",
    "node:build:system": "node ./build/build-system.js",
    "release:major": "npm run build:system && release-it major",
    "release:minor": "npm run build:system && release-it minor",
    "release:patch": "npm run build:system && release-it patch",
    "start": "npm-run-all --parallel styleguide theo:onchange theo dev",
    "styleguide": "vue-styleguidist server --config ./config/docs.config.js",
    "styleguide:build": "vue-styleguidist build --config ./config/docs.config.js",
    "test:unit": "vue-cli-service test:unit",
    "theo": "theo ./src/tokens/tokens.yml --transform web --format map.scss,scss,raw.json,json --dest ./src/assets/tokens",
    "theo:onchange": "onchange \"./src/tokens/*.yml\" -- npm run theo",
    "unit": "jest --config ./test/unit/jest.conf.js --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.16",
    "@fortawesome/free-solid-svg-icons": "^5.8.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "conventional-changelog-cli": "^2.0.12",
    "flatpickr": "^4.5.7",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "tinycolor2": "^1.4.1",
    "vue": "^2.6.10",
    "vue-add-to-calendar": "^1.0.4",
    "vue-clickaway": "^2.2.2",
    "vue-element-queries": "^0.3.0",
    "vue-fontawesome": "0.0.2",
    "vue-link": "^1.6.1",
    "vue-lodash": "^2.0.2",
    "vue-moment": "^4.0.0",
    "vue-responsive-components": "^0.2.3",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.4.2",
    "codemirror": "^5.45.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "node-notifier": "^5.4.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
