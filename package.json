    "portfinder": "^1.0.20",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "release-it": "^10.3.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "semver": "^5.6.0",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "theo": "^8.1.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
